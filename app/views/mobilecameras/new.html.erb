
<% provide(:title, 'Mobile Registration') %>
<h1>Registration</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @mobilecamera, local: true) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :cam_name %>
      <%= f.text_field :cam_name, class: 'form-control' %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.submit "Registration Mobilecamera", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
